                                if(val.wbs_code.includes(item.wbs_code+'.')){
                                    status = true;
                                }
                            });

                            if (status) {
                                $popup.confirm({
                                    caption: "Kegiatan : " + item.nama_kegiatan + " tidak dapat dihapus karena memiliki sub kegiatan.",
                                    buttons: [
                                        $button('yes', {
                                            onclick: function () {
                                                $popup.close(false);
                                            }
                                        })
                                    ]
                                });
